{
  "name": "@sisukas/chimes",
  "version": "1.0.9",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "yarn publish --access public"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Prasanth Janardanan",
  "module": "dist/chimes.esm.js",
  "size-limit": [
    {
      "path": "dist/chimes.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chimes.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "@types/b2a": "^1.0.0",
    "@types/qs": "^6.9.7",
    "husky": "^7.0.4",
    "size-limit": "^6.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "axios-extensions": "^3.1.3",
    "b2a": "^1.1.2",
    "qs": "^6.11.0"
  }
}
